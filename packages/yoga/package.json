{
  "name": "@gympass/yoga",
  "version": "5.2.0-alpha.4",
  "description": "Gympass component library",
  "main": "./src",
  "sideEffects": false,
  "scripts": {
    "build": "yarn build:web && yarn build:native && yarn build:esm",
    "build:web": "NODE_ENV=web babel ./src --out-dir dist/cjs",
    "build:esm": "NODE_ENV=esm babel ./src --out-dir dist/esm",
    "build:native": "NODE_ENV=native babel ./src --out-dir dist/cjs",
    "prebuild": "rm -rf ./dist",
    "postbuild": "cp -r ./src/Fonts ./dist/fonts",
    "prepublishOnly": "node ../../scripts/prepublish.js --rn"
  },
  "keywords": [
    "Gympass",
    "components",
    "styled-components",
    "react",
    "design-system"
  ],
  "homepage": "https://github.com/Gympass/yoga#readme",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gympass/yoga.git"
  },
  "bugs": {
    "url": "https://github.com/Gympass/yoga/issues"
  },
  "dependencies": {
    "@gympass/yoga-common": "^0.1.2-alpha.4",
    "@gympass/yoga-helpers": "^0.1.1-alpha.0",
    "@gympass/yoga-icons": "^0.11.2-alpha.5",
    "@gympass/yoga-tokens": "^1.1.0-alpha.0",
    "@ptomasroos/react-native-multi-slider": "^2.2.2",
    "downshift": "^6.1.0",
    "prop-types": "^15.7.2",
    "proxy-polyfill": "^0.3.2",
    "rc-slider": "^9.7.1",
    "react-google-font-loader": "^1.1.0"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-native": "^7.2.0",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "styled-components": "^5.2.1"
  },
  "peerDependencies": {
    "styled-components": "^5.2.1"
  }
}
